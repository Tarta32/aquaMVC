RewriteEngine On

RewriteRule ^([a-zA-Z0-9\-\/\_\?\=]*)$ index.php?chemin=$1/%{QUERY_STRING}

RewriteCond %{HTTP_HOST} ^aquavision-shooting.com$
RewriteRule ^(.*) http://www.aquavision-shooting.com//$1 [QSA,L,R=301]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ http://www.aquavision-shooting.com//$1 [R,L]
RewriteCond %{HTTP_HOST} !^http://www.aquavision-shooting.com/ [NC]
RewriteRule (.*) http://www.aquavision-shooting.com/$1 [QSA,R=301,L]
RewriteCond %{HTTPS} !=on
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

<IfModule mod_rewrite.c>
RewriteCond %{THE_REQUEST} (.*)index\.(php|htm|html)\ HTTP/
RewriteRule ^(.*)index\.(php|html|htm)$ $1 [R=301,NC,L]
</IfModule>

